<?xml version="1.0" standalone="yes" ?>
<xs:schema id="dsReinforcingAbility" targetNamespace="http://www.tempuri.org/dsReinforcingAbility.xsd"
	xmlns:mstns="http://www.tempuri.org/dsReinforcingAbility.xsd" xmlns="http://www.tempuri.org/dsReinforcingAbility.xsd"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata"
	attributeFormDefault="qualified" elementFormDefault="qualified">
	<xs:element name="dsReinforcingAbility" msdata:IsDataSet="true" msdata:Locale="en-ZA">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="JobRate">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="JobNo" type="xs:string" />
							<xs:element name="Rate" type="xs:decimal" minOccurs="0" />
							<xs:element name="TypeCode" type="xs:string" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="ProductType">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="TypeCode" type="xs:string" />
							<xs:element name="Weight" type="xs:double" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Company">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Address" type="xs:string" minOccurs="0" />
							<xs:element name="AddressLine2" type="xs:string" minOccurs="0" />
							<xs:element name="AddressLine3" type="xs:string" minOccurs="0" />
							<xs:element name="AddressLine4" type="xs:string" minOccurs="0" />
							<xs:element name="CompanyName" type="xs:string" minOccurs="0" />
							<xs:element name="CompanyNo" type="xs:string" />
							<xs:element name="Email" type="xs:string" minOccurs="0" />
							<xs:element name="Fax" type="xs:string" minOccurs="0" />
							<xs:element name="LastCutNum" type="xs:int" minOccurs="0" />
							<xs:element name="LastInvNum" type="xs:int" minOccurs="0" />
							<xs:element name="Message" type="xs:string" minOccurs="0" />
							<xs:element name="PostalCode" type="xs:int" minOccurs="0" />
							<xs:element name="RegNo" type="xs:string" minOccurs="0" />
							<xs:element name="Telephone" type="xs:string" minOccurs="0" />
							<xs:element name="UnitOfMeas" type="xs:string" minOccurs="0" />
							<xs:element name="VatNo" type="xs:string" minOccurs="0" />
							<xs:element name="VatPerc" type="xs:decimal" minOccurs="0" />
							<xs:element name="Website" type="xs:string" minOccurs="0" />
							<xs:element name="No_x0026_Name" msdata:ReadOnly="true" msdata:Expression="'[' + CompanyNo + ']' + '  ' + CompanyName"
								type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Contractor">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ActiveY_x002F_N" type="xs:boolean" minOccurs="0" />
							<xs:element name="AddressLine1" type="xs:string" minOccurs="0" />
							<xs:element name="AddressLine2" type="xs:string" minOccurs="0" />
							<xs:element name="AddressLine3" type="xs:string" minOccurs="0" />
							<xs:element name="AddressLine4" type="xs:string" minOccurs="0" />
							<xs:element name="ContractorName" type="xs:string" minOccurs="0" />
							<xs:element name="ContractorNo" type="xs:string" />
							<xs:element name="PostalCode" type="xs:int" minOccurs="0" />
							<xs:element name="Reg_x0020_No" type="xs:string" minOccurs="0" />
							<xs:element name="Telephone" type="xs:string" minOccurs="0" />
							<xs:element name="VAT_x0020_No" type="xs:string" minOccurs="0" />
							<xs:element name="No_x0026_Name" msdata:ReadOnly="true" msdata:Expression="'[' + ContractorNo + ']' + '  ' + ContractorName"
								type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="CuttingSheet">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CSHeading" type="xs:string" minOccurs="0" />
							<xs:element name="CutDate" type="xs:dateTime" minOccurs="0" />
							<xs:element name="CutSheetNo" type="xs:int" />
							<xs:element name="Details" type="xs:string" minOccurs="0" />
							<xs:element name="InvoiceNo" type="xs:int" minOccurs="0" />
							<xs:element name="Job_x0020_No" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Job">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="AddedDiscount" type="xs:decimal" minOccurs="0" />
							<xs:element name="CompanyNo" type="xs:string" minOccurs="0" />
							<xs:element name="ContractorNo" type="xs:string" minOccurs="0" />
							<xs:element name="Design" type="xs:decimal" minOccurs="0" />
							<xs:element name="Discount" type="xs:int" minOccurs="0" />
							<xs:element name="JobName" type="xs:string" minOccurs="0" />
							<xs:element name="JobNo" type="xs:string" />
							<xs:element name="OrderNo" type="xs:string" minOccurs="0" />
							<xs:element name="Tons_x0020_Or_x0020_Kilograms" type="xs:string" minOccurs="0" />
							<xs:element name="UnitOfMeas" type="xs:string" minOccurs="0" />
							<xs:element name="No_x0026_Name" msdata:ReadOnly="true" msdata:Expression="'[' + JobNo + ']' + '  ' + JobName"
								type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="SchedItem">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CutSheetNo" type="xs:int" />
							<xs:element name="ScheduleNo" type="xs:string" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="CutItem">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CutSheetNo" type="xs:int" />
							<xs:element name="Item" type="xs:int" />
							<xs:element name="Length" type="xs:double" minOccurs="0" nillable="0" />
							<xs:element name="Qty" type="xs:int" minOccurs="0" />
							<xs:element name="ScheduleNo" type="xs:string" />
							<xs:element name="TypeCode" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
		<xs:unique name="Constraint1" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:JobRate" />
			<xs:field xpath="mstns:JobNo" />
			<xs:field xpath="mstns:TypeCode" />
		</xs:unique>
		<xs:unique name="ProductType_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:ProductType" />
			<xs:field xpath="mstns:TypeCode" />
		</xs:unique>
		<xs:unique name="Company_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:Company" />
			<xs:field xpath="mstns:CompanyNo" />
		</xs:unique>
		<xs:unique name="Contractor_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:Contractor" />
			<xs:field xpath="mstns:ContractorNo" />
		</xs:unique>
		<xs:unique name="CuttingSheet_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:CuttingSheet" />
			<xs:field xpath="mstns:CutSheetNo" />
		</xs:unique>
		<xs:unique name="Job_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:Job" />
			<xs:field xpath="mstns:JobNo" />
		</xs:unique>
		<xs:unique name="SchedItem_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:SchedItem" />
			<xs:field xpath="mstns:CutSheetNo" />
			<xs:field xpath="mstns:ScheduleNo" />
		</xs:unique>
		<xs:unique name="CutItem_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:CutItem" />
			<xs:field xpath="mstns:CutSheetNo" />
			<xs:field xpath="mstns:Item" />
			<xs:field xpath="mstns:ScheduleNo" />
		</xs:unique>
	</xs:element>
</xs:schema>